											</telerikPrimitives:RadBorder>


										</ex:GridEx>

										<local:LabelEx Style="{StaticResource ThisTestRelatedLabelStyle}" Text="{converters:T ResourceId=ResultInterpretation}" />
										<telerikPrimitives:RadBorder
											Margin="0,2,5,5"
											Padding="5,2"
											BorderColor="#CCC"
											CornerRadius="3"
											BorderThickness="0">
											<ex:LabelEx
												Text="{Binding _ResultInterpretation}"
												Style="{StaticResource ThisTestRelatedEnterStyle}"
												LineBreakMode="WordWrap" />
										</telerikPrimitives:RadBorder>



									</local:StackLayoutEx>
								</ex:ScrollViewEx>
							</ex:AccordionItemEx.Content>
						</ex:AccordionItemEx>

					</ex:RadAccordionEx>
				</ex:GridEx>

				<ex:GridEx
					Grid.Row="1"
					IsVisible="{Binding IsShowListMode}"
					Margin="5">
					<ex:GridEx.ColumnDefinitions>
						<ColumnDefinition Width="*" />
					</ex:GridEx.ColumnDefinitions>

					<ex:RadButtonEx
						Grid.Column="0"
						Text="{converters:T ResourceId=AddPatient,
						                    ToUpper=True}"
						Style="{StaticResource ThisButtonStyle}"
						HeightRequest="{StaticResource HHH2}"
						BackgroundColor="{StaticResource GreenTextColor}"
						Command="{Binding PatientAddCommand}" />
				</ex:GridEx>

				<ex:GridEx
					Grid.Row="1"
					IsVisible="{Binding IsShowFromMode}"
					Margin="5">
					<ex:GridEx.ColumnDefinitions>
						<ColumnDefinition Width="*" />
						<ColumnDefinition Width="*" />
					</ex:GridEx.ColumnDefinitions>

					<ex:RadButtonEx
						Grid.Column="0"
						Text="{converters:T ResourceId=Delete,
						                    ToUpper=true}"
						IsVisible="{Binding IsNewRow, Converter={StaticResource InverseBoolConverter}}"
						Style="{StaticResource ThisButtonStyle}"
						HeightRequest="{StaticResource HHH2}"
						BackgroundColor="{StaticResource RedLightColor}"
						Command="{Binding PatientDeleteCommand}" />

					<ex:RadButtonEx
						Grid.Column="1"
						Text="{converters:T ResourceId=Submit,
						                    ToUpper=true}"
						Style="{StaticResource ThisButtonStyle}"
						HeightRequest="{StaticResource HHH2}"
						Command="{Binding CommitCommand}">
						<ex:RadButtonEx.Triggers>
							<Trigger TargetType="ex:RadButtonEx" Property="IsEnabled" Value="False">
								<Setter Property="BackgroundColor" Value="{StaticResource GreenLightTextColor}" />
								<Setter Property="BorderColor" Value="{StaticResource RedErrorBorderColor}" />
								<Setter Property="BorderWidth" Value="1" />
							</Trigger>
						</ex:RadButtonEx.Triggers>
					</ex:RadButtonEx>


				</ex:GridEx>

			</ex:GridEx>



		</local:GridEx>
	</local:AbsoluteLayoutForPage>
</local:ContentPageEx>
