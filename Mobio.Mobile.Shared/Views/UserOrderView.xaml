<?xml version="1.0" encoding="utf-8" ?>
<local:ContentPageEx
	x:Class="OneBuilder.Mobile.Views.UserOrderView"
	xmlns="http://xamarin.com/schemas/2014/forms"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:ex="clr-namespace:OneBuilder.Mobile"
	xmlns:behaviors="clr-namespace:OneBuilder.Mobile.Behaviors"
	xmlns:converters="clr-namespace:OneBuilder.Mobile.Converters"
	xmlns:views="clr-namespace:OneBuilder.Mobile.Views"
	xmlns:viewmodel="clr-namespace:OneBuilder.Mobile.ViewModels"
	xmlns:local="clr-namespace:OneBuilder.Mobile"
	xmlns:svg="clr-namespace:FFImageLoading.Svg.Forms;assembly=FFImageLoading.Svg.Forms"
	xmlns:ef="clr-namespace:AiForms.Effects"
	xmlns:expander="clr-namespace:Telerik.XamarinForms.Primitives.Expander;assembly=Telerik.XamarinForms.Primitives"
	xmlns:htmlLabel="clr-namespace:LabelHtml.Forms.Plugin.Abstractions;assembly=HtmlLabel.Forms.Plugin"
	xmlns:telerikPrimitives="clr-namespace:Telerik.XamarinForms.Primitives;assembly=Telerik.XamarinForms.Primitives"
	xmlns:telerikInput="clr-namespace:Telerik.XamarinForms.Input;assembly=Telerik.XamarinForms.Input"
	xmlns:telerikListViewCommands="clr-namespace:Telerik.XamarinForms.DataControls.ListView.Commands;assembly=Telerik.XamarinForms.DataControls"
	xmlns:telerikListView="clr-namespace:Telerik.XamarinForms.DataControls.ListView;assembly=Telerik.XamarinForms.DataControls"
	x:Name="mainPage">

	<local:ContentPageEx.Resources>
		<ResourceDictionary>
			<viewmodel:UserOrderViewModel_BorderColorConverter x:Key="BorderColorConverter" />

			<Style x:Key="ThisTestRelatedLabelStyle" TargetType="{x:Type ex:LabelEx}">
				<Setter Property="FontFamily" Value="{StaticResource SegoeUI}" />
				<Setter Property="FontAttributes" Value="Bold" />
				<Setter Property="FontSize" Value="16" />
				<Setter Property="TextColor" Value="{StaticResource BlackLightTextColor}" />
			</Style>

			<Style x:Key="ThisTestRelatedEnterStyle" TargetType="{x:Type ex:LabelEx}">
				<Setter Property="FontFamily" Value="{StaticResource SegoeUI}" />
				<Setter Property="FontAttributes" Value="Bold" />
				<Setter Property="FontSize" Value="16" />
				<Setter Property="TextColor" Value="{StaticResource BlackTextColor}" />
			</Style>


			<Style x:Key="ThisLabelStyle" TargetType="{x:Type ex:LabelEx}">
				<Setter Property="Margin" Value="2,0,2,5" />
				<Setter Property="FontFamily" Value="{StaticResource SegoeUI}" />
				<Setter Property="FontSize" Value="14" />
				<Setter Property="LetterSpacing" Value="0.07" />
				<Setter Property="TextColor" Value="{StaticResource BlackTextColor}" />
			</Style>

			<Style x:Key="ThisComboBoxStyle" TargetType="{x:Type ex:RadComboBoxEx}">
				<Setter Property="FontFamily" Value="{StaticResource SegoeUI}" />
				<Setter Property="FontSize" Value="18" />
				<Setter Property="BackgroundColor" Value="{StaticResource GreenBackColor}" />
				<Setter Property="BorderThickness" Value="0" />
				<Setter Property="Padding" Value="14,0" />
			</Style>

			<Style x:Key="ThisDatePickerLabelStyle" TargetType="{x:Type ex:LabelEx}">
				<Setter Property="FontFamily" Value="{StaticResource SegoeUI}" />
				<Setter Property="FontSize" Value="18" />
			</Style>


			<Style x:Key="ThisDatePickerStyle" TargetType="{x:Type ex:RadDatePickerEx}">
				<Setter Property="DisplayLabelStyle" Value="{StaticResource ThisDatePickerLabelStyle}" />
				<Setter Property="BackgroundColor" Value="{StaticResource GreenBackColor}" />
				<Setter Property="BorderThickness" Value="0" />
				<Setter Property="Padding" Value="10,0" />
			</Style>


			<Style x:Key="ThisEntryStyle" TargetType="{x:Type ex:RadEntryEx}">
				<Setter Property="FontFamily" Value="{StaticResource SegoeUI}" />
				<Setter Property="FontSize" Value="18" />
				<Setter Property="BackgroundColor" Value="{StaticResource GreenBackColor}" />
				<Setter Property="BorderStyle" Value="{StaticResource EntryBorderStyle}" />
				<Setter Property="Padding" Value="15,0" />
			</Style>

			<Style x:Key="ThisEntryBorderStyle" TargetType="{x:Type telerikPrimitives:RadBorder}">
				<Setter Property="Margin" Value="2,2,2,25" />
				<Setter Property="HeightRequest" Value="{StaticResource HHH}" />
				<Setter Property="CornerRadius" Value="9" />
			</Style>


			<Style x:Key="ThisButtonStyle" TargetType="ex:RadButtonEx">
				<Setter Property="Margin" Value="8,25,8,5" />
				<Setter Property="FontFamily" Value="{StaticResource SegoeUIBlack}" />
				<Setter Property="FontSize" Value="16" />
				<Setter Property="BackgroundColor" Value="{StaticResource GreenTextColor}" />
				<Setter Property="TextColor" Value="{StaticResource WhiteTextColor}" />
				<Setter Property="BorderRadius" Value="22" />
				<Setter Property="BorderWidth" Value="0" />
			</Style>

			<Style x:Key="ThisAccordionItemHeader" TargetType="telerikPrimitives:AccordionItemHeader">
				<Setter Property="BackgroundColor" Value="{StaticResource GreenBackColor}" />
			</Style>


			<Style x:Key="GreenStrong" TargetType="ex:LabelEx">
				<Setter Property="FontSize" Value="10" />
				<Setter Property="FontAttributes" Value="Bold" />
				<Setter Property="TextColor" Value="#3c763d" />
				<Setter Property="HorizontalTextAlignment" Value="Start" />
			</Style>
			<Style x:Key="GreenBorder" TargetType="ex:RadBorderEx">
				<Setter Property="Margin" Value="2" />
				<Setter Property="Padding" Value="10,3" />
				<Setter Property="BackgroundColor" Value="#dff0d8" />
				<Setter Property="BorderColor" Value="#d6e9c6" />
				<Setter Property="BorderThickness" Value="1" />
				<Setter Property="CornerRadius" Value="3" />
			</Style>

			<Style x:Key="IAuthorizeCheckBox" TargetType="ex:RadCheckBoxEx">
				<Setter Property="Margin" Value="5,5,5,5" />
				<Setter Property="VerticalOptions" Value="Center" />
				<Setter Property="CheckedColor" Value="{StaticResource GreenTextColor}" />
			</Style>

			<Style
				x:Key="IAuthorizeLabel"
				TargetType="ex:LabelEx"
				BasedOn="{StaticResource MainCellLabelStyle}">
				<Setter Property="Margin" Value="0,0,0,0" />
				<Setter Property="VerticalOptions" Value="Center" />
			</Style>

			<Style
				x:Key="YesCheckLabel"
				TargetType="ex:LabelEx"
				BasedOn="{StaticResource MainCellLabelStyle}">
				<Setter Property="Text" Value="Yes" />
				<Setter Property="TextColor" Value="{StaticResource GreenTextColor}" />
				<Setter Property="FontSize" Value="12" />
				<Setter Property="VerticalTextAlignment" Value="Center" />
			</Style>
			<Style
				x:Key="NoCheckLabel"
				TargetType="ex:LabelEx"
				BasedOn="{StaticResource MainCellLabelStyle}">
				<Setter Property="Text" Value="No" />
				<Setter Property="TextColor" Value="{StaticResource RedLightColor}" />
				<Setter Property="FontSize" Value="12" />
				<Setter Property="VerticalTextAlignment" Value="Center" />
			</Style>

			<Style
				x:Key="QuestionnaireLabel"
				TargetType="ex:LabelEx"
				BasedOn="{StaticResource MainCellLabelStyle}">
				<Setter Property="FontSize" Value="15" />
				<Setter Property="Margin" Value="0,5,5,5" />
				<Setter Property="TextColor" Value="#212121" />
				<Setter Property="VerticalTextAlignment" Value="Center" />
			</Style>

			<Style x:Key="YesCheckBox" TargetType="ex:RadCheckBoxEx">
				<Setter Property="Margin" Value="5,5,0,5" />
				<Setter Property="VerticalOptions" Value="Center" />
				<Setter Property="CheckedColor" Value="{StaticResource GreenTextColor}" />
			</Style>
			<Style x:Key="NoCheckBox" TargetType="ex:RadCheckBoxEx">
				<Setter Property="Margin" Value="5,5,0,5" />
				<Setter Property="VerticalOptions" Value="Center" />
				<Setter Property="CheckedColor" Value="{StaticResource RedLightColor}" />
			</Style>

			<Style x:Key="QuestionnaireBorder" TargetType="ex:RadBorderEx">
				<Setter Property="Margin" Value="0" />
				<Setter Property="BorderColor" Value="#C6C6C6" />
				<Setter Property="CornerRadius" Value="3" />
				<Setter Property="BorderThickness" Value="0" />
			</Style>

			<ControlTemplate x:Key="PatientHeaderControlTemplate">
				<ex:GridEx Margin="0,10" BindingContext="{TemplateBinding BindingContext}">
					<ex:GridEx.ColumnDefinitions>
						<ColumnDefinition Width="40" />
						<ColumnDefinition Width="*" />
					</ex:GridEx.ColumnDefinitions>
					<svg:SvgCachedImage
						Grid.Column="0"
						Source="error.svg"
						HorizontalOptions="Start"
						IsVisible="{Binding Path=HasError}"
						WidthRequest="16"
						HeightRequest="16" />
					<svg:SvgCachedImage
						Grid.Column="0"
						Source="success.svg"
						HorizontalOptions="Start"
						IsVisible="{Binding Path=HasError, Converter={StaticResource InverseBoolConverter}}"
						WidthRequest="16"
						HeightRequest="16" />

					<Label Grid.Column="1" Text="{Binding Source={RelativeSource AncestorType={x:Type ex:AccordionItemEx}}, Path=HeaderText}" />
				</ex:GridEx>

			</ControlTemplate>

			<Color x:Key="PatientButtonBorderColor">#333333</Color>
			<Color x:Key="PatientButtonBorderErrorColor">#A94442</Color>

			<Style x:Key="PatientButtonStyle" TargetType="ex:RadButtonEx">
				<Setter Property="Margin" Value="8,10,8,5" />
				<Setter Property="ContentLayout" Value="left,10" />
				<Setter Property="HeightRequest" Value="{StaticResource HHH}" />
				<Setter Property="FontSize" Value="13" />
				<Setter Property="BorderColor" Value="{StaticResource PatientButtonBorderColor}" />
				<Setter Property="BorderRadius" Value="8" />
				<Setter Property="BorderWidth" Value="1" />
			</Style>




		</ResourceDictionary>
	</local:ContentPageEx.Resources>

	<local:AbsoluteLayoutForPage>
		<local:GridEx>
			<local:GridEx.RowDefinitions>
				<RowDefinition Height="Auto" />
				<RowDefinition Height="*" />
			</local:GridEx.RowDefinitions>

			<views:NavigationBarView Grid.Row="0" />

			<ex:GridEx Grid.Row="1" Padding="10">
				<ex:GridEx.RowDefinitions>
					<RowDefinition Height="*" />
					<RowDefinition Height="Auto" />
				</ex:GridEx.RowDefinitions>

				<ex:RadListViewEx
					x:Name="PatientList"
					IsVisible="{Binding IsShowListMode}"
					Grid.Row="0"
					ItemsSource="{Binding PatientOrderItems}"
					SelectionMode="None">
					<ex:RadListViewEx.LayoutDefinition>
						<telerikListView:ListViewLinearLayout VerticalItemSpacing="20">
							<!--<telerikListView:ListViewLinearLayout.ItemLength>
								<OnPlatform
									x:TypeArguments="x:Double"
									iOS="400"
									Android="400" />
							</telerikListView:ListViewLinearLayout.ItemLength>-->
						</telerikListView:ListViewLinearLayout>
					</ex:RadListViewEx.LayoutDefinition>

					<!--<ex:RadListViewEx.LayoutDefinition>
						<telerikListView:ListViewGridLayout
							SpanCount="2"
							HorizontalItemSpacing="5"
							VerticalItemSpacing="5" />
					</ex:RadListViewEx.LayoutDefinition>-->

					<ex:RadListViewEx.ItemStyle>
						<telerikListView:ListViewItemStyle BorderColor="Transparent" BorderWidth="0" />
					</ex:RadListViewEx.ItemStyle>

					<ex:RadListViewEx.Commands>
						<telerikListViewCommands:ListViewUserCommand Id="ItemTap" Command="{Binding PatientItemTapCommand}" />
					</ex:RadListViewEx.Commands>

					<ex:RadListViewEx.ItemTemplate>
						<DataTemplate>
							<telerikListView:ListViewTemplateCell>
								<telerikListView:ListViewTemplateCell.View>
									<ex:GridEx>
										<ex:GridEx.ColumnDefinitions>
											<ColumnDefinition Width="60" />
											<ColumnDefinition Width="*" />
											<ColumnDefinition Width="80" />
										</ex:GridEx.ColumnDefinitions>
										<ex:GridEx.RowDefinitions>
											<RowDefinition Height="Auto" />
											<RowDefinition Height="Auto" />
										</ex:GridEx.RowDefinitions>

										<svg:SvgCachedImage
											Grid.Column="0"
											Grid.Row="0"
											Grid.RowSpan="2"
											Source="Patient.svg"
											HorizontalOptions="Center"
											VerticalOptions="Center"
											WidthRequest="48"
											HeightRequest="48" />

										<ex:LabelEx
											Grid.Column="1"
											Grid.Row="0"
											FontFamily="{StaticResource SegoeUI}"
											FontSize="18"
											Text="{Binding Patient.FullPatientName}"
											TextColor="{Binding Patient.TextColor}"
											HorizontalTextAlignment="Start"
											VerticalTextAlignment="Center" />
										<!--
											ef:SizeToFit.On="true"
											ef:SizeToFit.CanExpand="False"
										-->
										<ex:LabelEx
											Grid.Column="1"
											Grid.Row="1"
											FontFamily="{StaticResource SegoeUI}"
											FontSize="12"
											Text="{Binding InstitutionName}"
											TextColor="{Binding Patient.TextColor}"
											HorizontalTextAlignment="Start"
											VerticalTextAlignment="Center" />

										<ex:LabelEx
											Grid.Column="2"
											Grid.Row="0"
											Grid.RowSpan="2"
											Margin="3,0,0,0"
											FontFamily="{StaticResource SegoeUI}"
											FontSize="12"
											FontAttributes="Bold"
											Text="{Binding Patient.BirthDate, StringFormat='{0:MM/dd/yyyy}'}"
											TextColor="{Binding Patient.TextColor}"
											HorizontalTextAlignment="Start"
											VerticalTextAlignment="Center"
											VerticalOptions="Center" />

										<!--<telerikPrimitives:RadBorder
											Margin="2"
											Padding="2"
											HeightRequest="30"
											BackgroundColor="{Binding Patient.BackgroundColor}"
											BorderColor="{Binding Patient.BorderColor}"
											CornerRadius="7">
											<ex:LabelEx
												FontFamily="{StaticResource SegoeUI}"
												FontSize="16"
												Text="{Binding Patient.FullPatientName}"
												TextColor="{Binding Patient.TextColor}"
												HorizontalTextAlignment="Center"
												VerticalTextAlignment="Center"
												ef:SizeToFit.On="true"
												ef:SizeToFit.CanExpand="False" />
										</telerikPrimitives:RadBorder>-->

									</ex:GridEx>
								</telerikListView:ListViewTemplateCell.View>
							</telerikListView:ListViewTemplateCell>
						</DataTemplate>
					</ex:RadListViewEx.ItemTemplate>

					<ex:RadListViewEx.Behaviors>
						<behaviors:RadListViewScrollToBehavior ScrollTo="{Binding SelectedPatientOrderItemScrollToRow}" />
					</ex:RadListViewEx.Behaviors>
				</ex:RadListViewEx>

				<ex:GridEx
					x:Name="PatientDetails"
					Grid.Row="0"
					IsVisible="{Binding IsShowFromMode}">
					<ex:RadAccordionEx
						Margin="3"
						BindingContext="{Binding SelectedPatientOrderItem}"
						CanCollapseAllItems="True"
						AnimationDuration="500"
						AnimationEasing="SpringOut"
						Spacing="8">

						<ex:AccordionItemEx HeaderText="{converters:T ResourceId=General}" IsExpanded="{Binding PatientHeaderModels[General].IsExpanded, Source={RelativeSource AncestorType={x:Type viewmodel:UserOrderViewModel}}}">
							<ex:AccordionItemEx.Header>
								<telerikPrimitives:AccordionItemHeader Style="{StaticResource ThisAccordionItemHeader}">
									<ContentView ControlTemplate="{StaticResource PatientHeaderControlTemplate}" BindingContext="{Binding PatientHeaderModels[General], Source={RelativeSource AncestorType={x:Type viewmodel:UserOrderViewModel}}}" />
								</telerikPrimitives:AccordionItemHeader>
							</ex:AccordionItemEx.Header>
							<ex:AccordionItemEx.Content>
								<ex:ScrollViewEx>
									<ex:StackLayoutEx Padding="5">

										<ex:LabelEx
											Style="{StaticResource ThisLabelStyle}"
											Text="{converters:T ResourceId=FirstName,
											                    ToUpper=True}"
											Margin="2,10,2,5" />
										<telerikPrimitives:RadBorder Style="{StaticResource ThisEntryBorderStyle}" BorderColor="{Binding Path=ErrorsGeneral, ConverterParameter=FirstName, Source={RelativeSource AncestorType={x:Type viewmodel:UserOrderViewModel}}, Converter={StaticResource BorderColorConverter}}">
											<ex:RadEntryEx Text="{Binding Patient.FirstName}" Style="{StaticResource ThisEntryStyle}" />
										</telerikPrimitives:RadBorder>

										<ex:LabelEx Style="{StaticResource ThisLabelStyle}" Text="{converters:T ResourceId=LastName, ToUpper=True}" />
										<telerikPrimitives:RadBorder Style="{StaticResource ThisEntryBorderStyle}" BorderColor="{Binding Path=ErrorsGeneral, ConverterParameter=LastName, Source={RelativeSource AncestorType={x:Type viewmodel:UserOrderViewModel}}, Converter={StaticResource BorderColorConverter}}">
											<ex:RadEntryEx Text="{Binding Patient.LastName}" Style="{StaticResource ThisEntryStyle}" />
										</telerikPrimitives:RadBorder>

										<ex:LabelEx Style="{StaticResource ThisLabelStyle}" Text="{converters:T ResourceId=BirthDate, ToUpper=True}" />
										<telerikPrimitives:RadBorder
											Style="{StaticResource ThisEntryBorderStyle}"
											BackgroundColor="{StaticResource GreenBackColor}"
											BorderColor="{Binding Path=ErrorsGeneral, ConverterParameter=BirthDate, Source={RelativeSource AncestorType={x:Type viewmodel:UserOrderViewModel}}, Converter={StaticResource BorderColorConverter}}">
											<ex:RadDatePickerEx
												Style="{StaticResource ThisDatePickerStyle}"
												BackgroundColor="{StaticResource GreenBackColor}"
												Date="{Binding Patient.BirthDate}"
												MinimumDate="1901,01,01"
												DefaultHighlightedDate="2000,01,01"
												SpinnerFormat="MM/dd/yyyy" />
										</telerikPrimitives:RadBorder>

										<ex:LabelEx Style="{StaticResource ThisLabelStyle}" Text="{converters:T ResourceId=Gender, ToUpper=True}" />
										<telerikPrimitives:RadBorder Style="{StaticResource ThisEntryBorderStyle}" BackgroundColor="{StaticResource GreenBackColor}">
											<ex:RadComboBoxEx
												Style="{StaticResource ThisComboBoxStyle}"
												BackgroundColor="{StaticResource GreenBackColor}"
												SelectedItem="{Binding Patient.Gender, Mode=TwoWay}"
												ItemsSource="{Binding DdlGenders, Source={RelativeSource AncestorType={x:Type viewmodel:UserOrderViewModel}}}"
												Placeholder="Enter Gender" />
										</telerikPrimitives:RadBorder>

										<ex:LabelEx Style="{StaticResource ThisLabelStyle}" Text="{converters:T ResourceId=HealthcareCard, ToUpper=True}" />
										<telerikPrimitives:RadBorder Style="{StaticResource ThisEntryBorderStyle}">
											<ex:RadEntryEx Text="{Binding Patient.HealthCareCardNumber}" Style="{StaticResource ThisEntryStyle}" />
										</telerikPrimitives:RadBorder>

									</ex:StackLayoutEx>
								</ex:ScrollViewEx>
							</ex:AccordionItemEx.Content>
						</ex:AccordionItemEx>

						<ex:AccordionItemEx HeaderText="{converters:T ResourceId=ScreenQuestionnaire}">
							<ex:AccordionItemEx.Header>
								<telerikPrimitives:AccordionItemHeader Style="{StaticResource ThisAccordionItemHeader}">
									<ContentView ControlTemplate="{StaticResource PatientHeaderControlTemplate}" BindingContext="{Binding PatientHeaderModels[ScreenQuestionnaire], Source={RelativeSource AncestorType={x:Type viewmodel:UserOrderViewModel}}}" />
								</telerikPrimitives:AccordionItemHeader>
							</ex:AccordionItemEx.Header>
							<ex:AccordionItemEx.Content>
								<ex:ScrollViewEx Padding="5">
									<ex:GridEx>
										<Grid.RowDefinitions>
											<RowDefinition Height="Auto" />
											<RowDefinition Height="*" />
										</Grid.RowDefinitions>

										<ex:LabelEx
											Grid.Row="0"
											IsVisible="{Binding PatientHeaderModels[ScreenQuestionnaire].HasError, Source={RelativeSource AncestorType={x:Type viewmodel:UserOrderViewModel}}}"
											Style="{StaticResource ErrorLabelStyle}"
											Text="{converters:T ResourceId='(!)FillAllScreenQuestionnaire'}" />

										<ex:GridEx
											Grid.Row="1"
											Margin="0"
											RowSpacing="14"
											ColumnSpacing="20">
											<ex:GridEx.ColumnDefinitions>
												<ColumnDefinition Width="Auto" />
												<ColumnDefinition Width="*" />
											</ex:GridEx.ColumnDefinitions>
											<ex:GridEx.RowDefinitions>
												<RowDefinition Height="Auto" />
												<RowDefinition Height="Auto" />
												<RowDefinition Height="Auto" />
												<RowDefinition Height="Auto" />
												<RowDefinition Height="Auto" />
												<RowDefinition Height="Auto" />
												<RowDefinition Height="Auto" />
												<RowDefinition Height="Auto" />
												<RowDefinition Height="Auto" />
												<RowDefinition Height="Auto" />
												<RowDefinition Height="Auto" />
												<RowDefinition Height="Auto" />
											</ex:GridEx.RowDefinitions>

											<ex:RadBorderEx
												Grid.Row="0"
												Grid.Column="0"
												Grid.ColumnSpan="2"
												Style="{StaticResource QuestionnaireBorder}">
											</ex:RadBorderEx>
											<StackLayout
												Grid.Row="0"
												Grid.Column="0"
												Orientation="Horizontal">
												<ex:RadCheckBoxEx Style="{StaticResource YesCheckBox}" IsChecked="{Binding ScreenQuiz.FeverYes}" />
												<ex:LabelEx Style="{StaticResource YesCheckLabel}" />
												<ex:RadCheckBoxEx Style="{StaticResource NoCheckBox}" IsChecked="{Binding ScreenQuiz.FeverNo}" />
												<ex:LabelEx Style="{StaticResource NoCheckLabel}" />
											</StackLayout>
											<ex:LabelEx
												Grid.Row="0"
												Grid.Column="1"
												Text="{converters:T ResourceId=Fever,
												                    ToUpper=True}"
												Style="{StaticResource QuestionnaireLabel}" />

											<ex:RadBorderEx
												Grid.Row="1"
												Grid.Column="0"
												Grid.ColumnSpan="2"
												Style="{StaticResource QuestionnaireBorder}">
											</ex:RadBorderEx>
											<StackLayout
												Grid.Row="1"
												Grid.Column="0"
												Orientation="Horizontal">
												<ex:RadCheckBoxEx Style="{StaticResource YesCheckBox}" IsChecked="{Binding ScreenQuiz.RunnyNoiseYes}" />
												<ex:LabelEx Style="{StaticResource YesCheckLabel}" />
												<ex:RadCheckBoxEx Style="{StaticResource NoCheckBox}" IsChecked="{Binding ScreenQuiz.RunnyNoiseNo}" />
												<ex:LabelEx Style="{StaticResource NoCheckLabel}" />
											</StackLayout>
											<ex:LabelEx
												Grid.Row="1"
												Grid.Column="1"
												Text="{converters:T ResourceId=RunnyNose,
												                    ToUpper=True}"
												Style="{StaticResource QuestionnaireLabel}" />


											<ex:RadBorderEx
												Grid.Row="2"
												Grid.Column="0"
												Grid.ColumnSpan="2"
												Style="{StaticResource QuestionnaireBorder}">
											</ex:RadBorderEx>
											<StackLayout
												Grid.Row="2"
												Grid.Column="0"
												Orientation="Horizontal">
												<ex:RadCheckBoxEx Style="{StaticResource YesCheckBox}" IsChecked="{Binding ScreenQuiz.DifficultyBreathingYes}" />
												<ex:LabelEx Style="{StaticResource YesCheckLabel}" />
												<ex:RadCheckBoxEx Style="{StaticResource NoCheckBox}" IsChecked="{Binding ScreenQuiz.DifficultyBreathingNo}" />
												<ex:LabelEx Style="{StaticResource NoCheckLabel}" />
											</StackLayout>
											<ex:LabelEx
												Grid.Row="2"
												Grid.Column="1"
												Text="{converters:T ResourceId=DifficultyBreathing,
												                    ToUpper=True}"
												Style="{StaticResource QuestionnaireLabel}" />

											<ex:RadBorderEx
												Grid.Row="3"
												Grid.Column="0"
												Grid.ColumnSpan="2"
												Style="{StaticResource QuestionnaireBorder}">
											</ex:RadBorderEx>
											<StackLayout
												Grid.Row="3"
												Grid.Column="0"
												Orientation="Horizontal">
												<ex:RadCheckBoxEx Style="{StaticResource YesCheckBox}" IsChecked="{Binding ScreenQuiz.CoughYes}" />
												<ex:LabelEx Style="{StaticResource YesCheckLabel}" />
												<ex:RadCheckBoxEx Style="{StaticResource NoCheckBox}" IsChecked="{Binding ScreenQuiz.CoughNo}" />
												<ex:LabelEx Style="{StaticResource NoCheckLabel}" />
											</StackLayout>
											<ex:LabelEx
												Grid.Row="3"
												Grid.Column="1"
												Text="{converters:T ResourceId=Cough,
												                    ToUpper=True}"
												Style="{StaticResource QuestionnaireLabel}" />

											<ex:RadBorderEx
												Grid.Row="4"
												Grid.Column="0"
												Grid.ColumnSpan="2"
												Style="{StaticResource QuestionnaireBorder}">
											</ex:RadBorderEx>
											<StackLayout
												Grid.Row="4"
												Grid.Column="0"
												Orientation="Horizontal">
												<ex:RadCheckBoxEx Style="{StaticResource YesCheckBox}" IsChecked="{Binding ScreenQuiz.LossOfTasteYes}" />
												<ex:LabelEx Style="{StaticResource YesCheckLabel}" />
												<ex:RadCheckBoxEx Style="{StaticResource NoCheckBox}" IsChecked="{Binding ScreenQuiz.LossOfTasteNo}" />
												<ex:LabelEx Style="{StaticResource NoCheckLabel}" />
											</StackLayout>
											<ex:LabelEx
